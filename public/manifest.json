{
  "name": "Needyfy - Hire & Rent Anything",
  "short_name": "Needyfy",
  "id": "/",
  "description": "The ultimate platform for hiring and renting equipment, tools, and services",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#3b82f6",
  "orientation": "portrait-primary",
  "scope": "/",
  "categories": ["productivity", "business", "utilities"],
  "lang": "en",
  "dir": "ltr",
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "icons": [
    {
      "src": "/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "shortcuts": [
    {
      "name": "Browse Equipment",
      "short_name": "Browse",
      "description": "Browse available equipment for rent",
      "url": "/equipment",
      "icons": [
        {
          "src": "/icon-192x192.png",
          "sizes": "192x192"
        }
      ]
    },
    {
      "name": "List Equipment",
      "short_name": "List",
      "description": "List your equipment for rent",
      "url": "/list-equipment",
      "icons": [
        {
          "src": "/icon-192x192.png",
          "sizes": "192x192"
        }
      ]
    }
  ],
  "screenshots": [
    {
      "src": "/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Needyfy Equipment Rental Platform"
    }
  ],
  "iarc_rating_id": "e84b072d-71de-5b85-bcbc-38761d0fcec5",
  "prefer_related_applications": false,
  "related_applications": [],
  "scope_extensions": [
    {
      "origin": "https://needyfy.com"
    }
  ],
  "display_override": ["window-controls-overlay", "standalone"],
  "handle_links": "preferred",
  "file_handlers": [
    {
      "action": "/equipment/upload",
      "accept": {
        "image/*": [".jpg", ".jpeg", ".png", ".webp"],
        "application/pdf": [".pdf"]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+needyfy",
      "url": "/equipment/%s"
    }
  ],
  "share_target": {
    "action": "/equipment/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "images",
          "accept": ["image/*"]
        }
      ]
    }
  },
  "edge_side_panel": {
    "preferred_width": 412
  }
}