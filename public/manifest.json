  "related_applications": [],
  "scope_extensions": [
    {
      "origin": "https://needyfy.com"
    }
  ],
  "display_override": ["window-controls-overlay", "standalone"],
  "handle_links": "preferred",
  "file_handlers": [
    {
      "action": "/equipment/upload",
      "accept": {
        "image/*": [".jpg", ".jpeg", ".png", ".webp"],
        "application/pdf": [".pdf"]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+needyfy",
      "url": "/equipment/%s"
    }
  ],
  "share_target": {
    "action": "/equipment/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "images",
          "accept": ["image/*"]
        }
      ]
    }
  },
